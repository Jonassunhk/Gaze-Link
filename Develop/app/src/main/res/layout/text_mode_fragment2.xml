<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_500"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/textInput2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/beige"
        android:fontFamily="sans-serif-medium"
        android:padding="7sp"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintHorizontal_bias="0.525"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.395"
        app:layout_constraintWidth_percent="0.942"
        tools:text="This is the user inputted text." />

    <EditText
        android:id="@+id/contextBox2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autofillHints="Enter context here:"
        android:background="@color/metal_green"
        android:fontFamily="sans-serif-medium"
        android:hint="Enter context here"
        android:inputType="textCapSentences|textAutoCorrect"
        android:padding="7sp"
        android:textAllCaps="false"
        android:textColor="@color/columbia_blue"
        android:textColorHint="@color/purple_200"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.2"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566"
        app:layout_constraintWidth_percent="0.475" />

    <TextView
        android:id="@+id/sentenceBox2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/teal_700"
        android:fontFamily="sans-serif-medium"
        android:padding="7sp"
        android:textAllCaps="false"
        android:textColor="@color/ghost_blue"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.2"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566"
        app:layout_constraintWidth_percent="0.47"
        tools:text="This is the sentence users generate with sentence prediction" />

    <TextView
        android:id="@+id/upGazeText2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/ghost_blue"
        android:fontFamily="sans-serif-medium"
        android:paddingStart="15dp"
        android:paddingTop="35dp"
        android:paddingEnd="15dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.26"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.069"
        app:layout_constraintWidth_percent="0.44"
        tools:text="ABCDEF" />

    <TextView
        android:id="@+id/closedGazeText2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/ghost_blue"
        android:fontFamily="sans-serif-medium"
        android:paddingTop="13dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.93"
        app:layout_constraintWidth_percent="0.44"
        tools:text="Speak sentence" />

    <TextView
        android:id="@+id/leftUpGazeText2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/ghost_blue"
        android:fontFamily="sans-serif-medium"
        android:paddingStart="10dp"
        android:paddingTop="30dp"
        android:paddingEnd="10dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.26"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.069"
        app:layout_constraintWidth_percent="0.24"
        tools:text="ABCDEF" />

    <TextView
        android:id="@+id/rightUpGazeText2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/ghost_blue"
        android:fontFamily="sans-serif-medium"
        android:paddingStart="10dp"
        android:paddingTop="30dp"
        android:paddingEnd="10dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.26"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.069"
        app:layout_constraintWidth_percent="0.245"
        tools:text="ABCDEF" />

    <TextView
        android:id="@+id/leftGazeText2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/ghost_blue"
        android:fontFamily="sans-serif-medium"
        android:paddingStart="15dp"
        android:paddingTop="20dp"
        android:paddingEnd="15dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.26"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.93"
        app:layout_constraintWidth_percent="0.24
"
        tools:text="ABCDEF" />

    <TextView
        android:id="@+id/rightGazeText2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/ghost_blue"
        android:fontFamily="sans-serif-medium"
        android:paddingStart="15dp"
        android:paddingTop="20dp"
        android:paddingEnd="15dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.26"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.93"
        app:layout_constraintWidth_percent="0.245"
        tools:text="ABCDEF" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_weight="5"
        android:contentDescription="@string/app_name"
        android:padding="2sp"
        android:src="@drawable/upgaze"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_weight="5"
        android:contentDescription="@string/app_name"
        android:padding="2sp"
        android:src="@drawable/closedgaze"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.499"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_weight="5"
        android:contentDescription="@string/app_name"
        android:padding="2sp"
        android:src="@drawable/leftupgaze"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_weight="5"
        android:contentDescription="@string/app_name"
        android:padding="2sp"
        android:src="@drawable/rightupgaze"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_weight="5"
        android:contentDescription="@string/app_name"
        android:padding="2sp"
        android:src="@drawable/rightgaze"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.996" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_weight="5"
        android:contentDescription="@string/app_name"
        android:padding="2sp"
        android:src="@drawable/leftgaze"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.996" />
</androidx.constraintlayout.widget.ConstraintLayout>